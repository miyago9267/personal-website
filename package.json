{
  "name": "myg-official-site",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "optimize": "bunx --bun vite optimize",
    "lint": "bunx --bun eslint \"src/**/*.{ts,vue}\"",
    "lint:fix": "bunx --bun eslint \"src/**/*.{ts,vue}\" --fix",
    "typecheck": "bunx --bun vue-tsc --noEmit"
  },
  "dependencies": {
    "unocss": "^0.58.0",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "vue-eslint-parser": "^9.3.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25"
  }
}
